@model PPcore.Models.mem_reward

<form id="mem_rewardForm" class="form-horizontal">
<div class="form-group">
    <label asp-for="reward_desc" class="col-md-3 control-label"></label>
    <div class="col-md-9">
        <textarea asp-for="reward_desc" class="form-control" rows="3"></textarea>
        <span asp-validation-for="reward_desc" class="text-danger" />
    </div>
</div>

</form>
<div class="form-group">
    <div class="col-md-offset-3 col-md-9">
        <button id="mem_rewardSubmit" class="btn btn-success">บันทึก</button>
    </div>
</div>
