@model PPcore.Models.mem_testcenter

@{
    Layout = null;
}

<form id="a_form" class="form-horizontal">
    <input asp-for="id" /><input asp-for="rowversion" /><input asp-for="mem_testcenter_code" />
    <input asp-for="CreatedBy" /><input asp-for="CreatedDate" />
    <div class="form-group">
        <label asp-for="mem_testcenter_desc" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="mem_testcenter_desc" class="form-control" />
            <span asp-validation-for="mem_testcenter_desc" class="text-danger" id="mem_testcenter_desc_msg" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="x_status" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <select asp-for="x_status" class="form-control" asp-items="@Html.x_status_SelectList()"></select>
        </div>
    </div>

</form>
<div class="form-group">
    <div class="col-md-offset-3 col-md-3">
        <div id="a_submit" class="btn btn-default btn-panel-yellow">&nbsp;&nbsp;บันทึก&nbsp;&nbsp;</div>
    </div>
    <div class="col-md-6">
        <div class="btn btn-default btn-panel-yellow pull-right" data-dismiss="modal" aria-hidden="true">&nbsp;&nbsp;&nbsp;ปิด&nbsp;&nbsp;&nbsp;</div>
    </div>
</div>

