@model PPcore.Models.project_course
@{
    Layout = "";
}

<form id="a_form" class="form-horizontal">
    <input asp-for="id" />
    <div class="form-group">
        <label asp-for="course_code" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="course_code" class="form-control" readonly />
            <span asp-validation-for="course_code" class="text-danger" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="course_desc" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <textarea asp-for="course_desc" class="form-control" readonly rows="3"></textarea>
            <span asp-validation-for="course_desc" class="text-danger" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="course_begin" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="course_begin" class="form-control" readonly />
            <span asp-validation-for="course_begin" class="text-danger" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="course_end" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="course_end" class="form-control" readonly />
            <span asp-validation-for="course_end" class="text-danger" />
        </div>
    </div>

</form>
<div class="form-group">
    <div class="col-md-offset-3 col-md-3">
        <div id="a_submit" class="btn btn-default btn-panel-yellow">&nbsp;&nbsp;ลงทะเบียนเข้าอบรม&nbsp;&nbsp;</div>
    </div>
    @*<div class="col-md-6">
        <div class="btn btn-default btn-panel-yellow pull-right" data-dismiss="modal" aria-hidden="true">&nbsp;&nbsp;&nbsp;ปิด&nbsp;&nbsp;&nbsp;</div>
    </div>*@
</div>

